package horse_move;

import java.util.Scanner;

public class Horse_Move {
	
	static Scanner sc = new Scanner(System.in);
	
	static int firstRow;
	static int firstCol;
	static int row; //zadavame granici na duskata po
	static int col; //koqto shte se dviji konq
	static char[][] board;
	static char horseFirstPosition = '*';
	static int[][] horseMoves = {{1,-2},
								 {2,-1},
								 {2,1},
								 {1,2},
								 {-1,2},
								 {-2,1},
								 {-2,-1},
								 {-1,-2}};
	
	public static void main(String[] args) {
		
		do{
			row = sc.nextInt();
			if(row == 2){
				do{
					col = sc.nextInt();
				}while(col < row + 1);
			}else{
				do{
					col = sc.nextInt();
				}while(col < 2);
			}
		}while(row < 2);
		
		
		
		board = new char[row][col];
		
		for(int i = 0; i < row; i++){
			for(int j = 0; j < col; j++){
				board[i][j] = ' ';
			}
		}
		
		printBoard();
		do{
			firstRow = sc.nextInt();
			firstCol = sc.nextInt();
		}while((firstRow < 0 || firstRow > row - 1) && (firstCol < 0 || firstCol > col - 1));
		board[firstRow][firstCol] = horseFirstPosition;
		
//		horseMove(firstRow, firstCol, horseMoves);
		
		printBoard();
		
	}
	
	static void printBoard() {
		for (int i = 0; i < row; i++) {
			for (int j = 0; j < col; j++) {
				if (j == 0) {
					System.out.print("| ");
				}
				System.out.print(board[i][j] + " | ");
			}
			System.out.println();
		}
		
	}
//	
//	static void horseMove(int n, int m, int [][] horseMoves){
//		
//		if(first)
//		if(board[n][m] == horseFirstPosition){
//			return;
//		}
//		if
//		
//		
//	}
	
}