package horse_move;

import java.util.Scanner;

public class Horse_Move {
	
	static Scanner sc = new Scanner(System.in);
	
	static int n; //zadavame granici na duskata po
	static int m; //koqto shte se dviji konq
	static char[][] board;
	
	public static void main(String[] args) {
		
		do{
			n = sc.nextInt();
		}while(n < 0);
		
		do{
			m = sc.nextInt();
		}while(m < 0);
		
		board = new char[n][m];
		
		for(int i = 0; i < n; i++){
			for(int j = 0; j < m; j++){
				board[i][j] = '_';
			}
		}
		
//		printBoard();
		
	}
	
//	static void printBoard() {
//		for (int i = 0; i < n; i++) {
//			for (int j = 0; j < m; j++) {
//				if (j == 0) {
//					System.out.print("| ");
//				}
//				System.out.print(board[i][j] + " | ");
//			}
//		}
//		System.out.println();
//	}
	
}